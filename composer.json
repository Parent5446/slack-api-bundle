    "autoload": {
        "psr-4": { "CastlePointAnime\\SlackApiBundle\\": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "guzzlehttp/guzzle-services": "0.5.0",
        "symfony/event-dispatcher": "~2.6",
        "symfony/config": "~2.6",
